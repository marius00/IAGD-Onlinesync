{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "PrivacyPolicy": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/privacy"
      },
      "backendUri": "http://grimdawn.dreamcrash.org/ia/privacy.html"
    },
    "Home": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/app"
      },
      "backendUri": "http://grimdawn.dreamcrash.org/ia/index.php"
    },
    "Login": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/login"
      },
      "backendUri": "https://iagd.azurewebsites.net/api/Authenticate?code=KjKyLhI0GQ5YHK/qGNOYKUibkwLh3cSv5bCa2uEMvx0VFKLirLYKEQ=="
    },
    "LoginOnSlash": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "backendUri": "https://iagd.azurewebsites.net/api/Authenticate?code=KjKyLhI0GQ5YHK/qGNOYKUibkwLh3cSv5bCa2uEMvx0VFKLirLYKEQ=="
    },
    "v2": {
      "debug": true,
      "matchCondition": {
        "route": "/api/v2/{*restOfPath}"
      },
      "backendUri": "https://localhost/v2_{restOfPath}"
    }
  }
}